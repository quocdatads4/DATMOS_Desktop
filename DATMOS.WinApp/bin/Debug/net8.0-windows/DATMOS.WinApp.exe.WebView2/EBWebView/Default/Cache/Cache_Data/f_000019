    <!-- Core JS from Customer Layout -->
    <script src="/vendor/libs/jquery/jquery.js" asp-append-version="true"></script>
    <script src="/vendor/js/bootstrap.js" asp-append-version="true"></script>
    <script src="/vendor/libs/perfect-scrollbar/perfect-scrollbar.js" asp-append-version="true"></script>
    <script src="/js/main.js" asp-append-version="true"></script>
    
    <!-- GMetrix Footer Scripts - Improved Version -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Footer Toggle Logic
            const toggleBtn = document.getElementById('toggleFooter');
            const footer = document.getElementById('gmetrixFooter');
            const body = document.body;
            const toastContainer = document.getElementById('toast-container');
            
            if (toggleBtn && footer) {
                toggleBtn.addEventListener('click', function() {
                    footer.classList.toggle('collapsed');
                    const icon = this.querySelector('i');
                    
                    if (footer.classList.contains('collapsed')) {
                        icon.classList.remove('fa-chevron-down');
                        icon.classList.add('fa-chevron-up');
                        body.style.paddingBottom = '60px';
                        if (toastContainer) toastContainer.style.bottom = '70px';
                    } else {
                        icon.classList.remove('fa-chevron-up');
                        icon.classList.add('fa-chevron-down');
                        body.style.paddingBottom = '260px';
                        if (toastContainer) toastContainer.style.bottom = '280px';
                    }
                });
            }

            // Toast Notification System
            function showToast(message, type = 'info', duration = 4000) {
                const toastContainer = document.getElementById('toast-container') || createToastContainer();
                const toast = document.createElement('div');
                toast.className = `toast ${type}`;
                toast.innerHTML = `
                    <i class="fas ${type === 'success' ? 'fa-check-circle' : type === 'warning' ? 'fa-exclamation-triangle' : 'fa-info-circle'}"></i>
                    <span>${message}</span>
                `;
                
                toastContainer.appendChild(toast);
                
                // Trigger animation
                setTimeout(() => toast.classList.add('show'), 10);
                
                // Auto remove
                setTimeout(() => {
                    toast.classList.remove('show');
                    setTimeout(() => {
                        if (toast.parentNode) {
                            toast.parentNode.removeChild(toast);
                        }
                    }, 400);
                }, duration);
                
                // Click to dismiss
                toast.addEventListener('click', () => {
                    toast.classList.remove('show');
                    setTimeout(() => {
                        if (toast.parentNode) {
                            toast.parentNode.removeChild(toast);
                        }
                    }, 400);
                });
            }
            
            function createToastContainer() {
                const container = document.createElement('div');
                container.id = 'toast-container';
                container.className = 'toast-container';
                document.body.appendChild(container);
                return container;
            }
            
            // Progress indicator - sử dụng phần tử đã có trong HTML
            function updateProgressIndicator(current, total) {
                const progressEl = document.getElementById('progress-indicator');
                if (progressEl) {
                    progressEl.innerHTML = `
                        <span>Tiến độ: ${current}/${total}</span>
                        <div class="progress-bar">
                            <div class="progress-fill" style="width: ${(current/total)*100}%"></div>
                        </div>
                    `;
                }
            }
            
            // Tab switching with animation
            document.querySelectorAll('.tab').forEach(tab => {
                tab.addEventListener('click', function() {
                    const oldActive = document.querySelector('.tab.active');
                    if (oldActive === this) return;
                    
                    // Animation
                    oldActive.classList.remove('active');
                    this.classList.add('active');
                    
                    // Update instruction with fade effect
                    const tabId = this.getAttribute('data-tab');
                    updateInstruction(tabId);
                    
                    // Show toast for tab change
                    const tabNames = {
                        'overview': 'Tổng quan',
                        'task1': 'Câu 1',
                        'task2': 'Câu 2', 
                        'task3': 'Câu 3',
                        'task4': 'Câu 4',
                        'task5': 'Câu 5'
                    };
                    showToast(`Đã chuyển đến ${tabNames[tabId]}`, 'info', 2000);
                });
            });
            
            // Button event handlers with loading states
            document.getElementById('prev-task').addEventListener('click', function() {
                const btn = this;
                btn.classList.add('loading');
                setTimeout(() => {
                    btn.classList.remove('loading');
                    showToast('Đã chuyển đến câu trước', 'success');
                }, 800);
            });
            
            document.getElementById('next-task').addEventListener('click', function() {
                const btn = this;
                btn.classList.add('loading');
                setTimeout(() => {
                    btn.classList.remove('loading');
                    showToast('Đã chuyển đến câu tiếp theo', 'success');
                }, 800);
            });
            
            document.getElementById('mark-completed').addEventListener('click', function() {
                const btn = this;
                btn.classList.add('loading');
                setTimeout(() => {
                    btn.classList.remove('loading');
                    showToast('Đã đánh dấu hoàn thành', 'success');
                }, 800);
            });
            
            document.getElementById('mark-review').addEventListener('click', function() {
                const btn = this;
                btn.classList.add('loading');
                setTimeout(() => {
                    btn.classList.remove('loading');
                    showToast('Đã đánh dấu cần xem lại', 'warning');
                }, 800);
            });
            
            document.getElementById('help-btn').addEventListener('click', function() {
                showToast('Đang tải trợ giúp...', 'info');
            });
            
            // Top link handlers
            document.getElementById('summary-link').addEventListener('click', function() {
                showToast('Đang tải tổng quan...', 'info');
            });
            
            document.getElementById('restart-link').addEventListener('click', function() {
                if (confirm('Bạn có chắc muốn khởi động lại bài thi? Mọi tiến độ chưa lưu sẽ bị mất.')) {
                    showToast('Đang khởi động lại bài thi...', 'warning', 3000);
                }
            });
            
            document.getElementById('save-link').addEventListener('click', function() {
                const btn = this;
                const originalText = btn.innerHTML;
                btn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Đang lưu...';
                btn.disabled = true;
                
                setTimeout(() => {
                    btn.innerHTML = originalText;
                    btn.disabled = false;
                    showToast('Đã lưu tiến độ thành công', 'success');
                }, 1500);
            });
            
            document.getElementById('grade-link').addEventListener('click', function() {
                const btn = this;
                btn.classList.add('loading');
                setTimeout(() => {
                    btn.classList.remove('loading');
                    showToast('Đang chấm điểm... Vui lòng đợi', 'info', 3000);
                }, 800);
            });
            
            // Timer with countdown simulation
            let remainingTime = 20 * 60 + 49; // 20:49 in seconds
            function updateTimer() {
                const timerEl = document.getElementById('gmetrix-timer');
                if (timerEl) {
                    remainingTime--;
                    
                    if (remainingTime <= 0) {
                        timerEl.textContent = '00:00:00';
                        timerEl.style.color = '#dc3545';
                        timerEl.style.fontWeight = 'bold';
                        showToast('Thời gian đã hết!', 'warning', 5000);
                        return;
                    }
                    
                    const hours = Math.floor(remainingTime / 3600);
                    const minutes = Math.floor((remainingTime % 3600) / 60);
                    const seconds = remainingTime % 60;
                    
                    const timeStr = `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
                    timerEl.textContent = timeStr;
                    
                    // Warning colors
                    if (remainingTime <= 300) { // 5 minutes
                        timerEl.style.color = '#ff9800';
                    } else if (remainingTime <= 60) { // 1 minute
                        timerEl.style.color = '#dc3545';
                        timerEl.classList.add('pulse');
                    }
                }
            }
            
            // Update timer every second
            const timerInterval = setInterval(updateTimer, 1000);
            updateTimer();
            
            // Keyboard navigation
            document.addEventListener('keydown', function(e) {
                // Tab navigation with arrow keys
                if (e.key === 'ArrowRight' || e.key === 'ArrowLeft') {
                    const tabs = Array.from(document.querySelectorAll('.tab'));
                    const currentIndex = tabs.findIndex(tab => tab.classList.contains('active'));
                    
                    if (e.key === 'ArrowRight' && currentIndex < tabs.length - 1) {
                        tabs[currentIndex + 1].click();
                        tabs[currentIndex + 1].focus();
                    } else if (e.key === 'ArrowLeft' && currentIndex > 0) {
                        tabs[currentIndex - 1].click();
                        tabs[currentIndex - 1].focus();
                    }
                }
                
                // Space/Enter for buttons
                if (e.key === ' ' || e.key === 'Enter') {
                    const focused = document.activeElement;
                    if (focused.classList.contains('btn') || focused.classList.contains('tab')) {
                        focused.click();
                        e.preventDefault();
                    }
                }
                
                // Shortcuts
                if (e.ctrlKey || e.metaKey) {
                    switch(e.key) {
                        case 's':
                            e.preventDefault();
                            document.getElementById('save-link').click();
                            break;
                        case 'r':
                            e.preventDefault();
                            document.getElementById('restart-link').click();
                            break;
                        case 'g':
                            e.preventDefault();
                            document.getElementById('grade-link').click();
                            break;
                    }
                }
            });
            
            // Add tabindex for keyboard navigation
            document.querySelectorAll('.btn, .tab, .top-link').forEach(el => {
                el.setAttribute('tabindex', '0');
            });
            
            // Add ARIA labels for accessibility
            document.querySelectorAll('.tab').forEach((tab, index) => {
                tab.setAttribute('aria-label', `Câu ${index === 0 ? 'Tổng quan' : index}`);
            });
            
            document.getElementById('prev-task').setAttribute('aria-label', 'Câu trước');
            document.getElementById('next-task').setAttribute('aria-label', 'Câu tiếp theo');
            document.getElementById('mark-completed').setAttribute('aria-label', 'Đánh dấu hoàn thành');
            document.getElementById('mark-review').setAttribute('aria-label', 'Đánh dấu cần xem lại');
            document.getElementById('help-btn').setAttribute('aria-label', 'Trợ giúp');
            
            function updateInstruction(tabId) {
                const instructions = {
                    'overview': 'Xem tổng quan tất cả các câu hỏi trong bài thi.',
                    'task1': 'Dưới tiêu đề <strong>Landscaping Made Easy</strong>, chèn một ảnh chụp màn hình của bức ảnh hiển thị trên tài liệu <strong>Project</strong>.',
                    'task2': 'Định dạng tiêu đề "Landscaping Made Easy" với kiểu chữ <strong>Title</strong>.',
                    'task3': 'Chèn một bảng với 3 cột và 4 hàng bên dưới đoạn văn đầu tiên.',
                    'task4': 'Áp dụng kiểu <strong>Grid Table 4 - Accent 1</strong> cho bảng vừa tạo.',
                    'task5': 'Căn giữa bảng và thêm chú thích "Landscaping Costs" phía trên bảng.'
                };
                
                const instructionEl = document.getElementById('instruction-text');
                if (instructionEl && instructions[tabId]) {
                    // Fade effect
                    instructionEl.style.opacity = '0.5';
                    setTimeout(() => {
                        instructionEl.innerHTML = instructions[tabId];
                        instructionEl.style.opacity = '1';
                    }, 200);
                }
            }
            
            // Project select functionality
            const projectSelect = document.getElementById('project-select');
            if (projectSelect) {
                projectSelect.addEventListener('change', function() {
                    const projectNumber = this.value;
                    const selectedOption = this.options[this.selectedIndex];
                    const projectText = selectedOption.text;
                    
                    // Show toast notification
                    showToast(`Đã chuyển đến ${projectText}`, 'info', 2000);
                    
                    // Update progress indicator based on project
                    const projectProgress = {
                        '1': { current: 1, total: 5 },
                        '2': { current: 1, total: 3 },
                        '3': { current: 1, total: 7 }
                    };
                    
                    if (projectProgress[projectNumber]) {
                        updateProgressIndicator(
                            projectProgress[projectNumber].current,
                            projectProgress[projectNumber].total
                        );
                    }
                    
                    // Update instruction based on project
                    const projectInstructions = {
                        '1': 'Dưới tiêu đề <strong>Landscaping Made Easy</strong>, chèn một ảnh chụp màn hình của bức ảnh hiển thị trên tài liệu <strong>Project</strong>.',
                        '2': 'Định dạng tiêu đề "Landscaping Made Easy" với kiểu chữ <strong>Title</strong>.',
                        '3': 'Chèn một bảng với 3 cột và 4 hàng bên dưới đoạn văn đầu tiên.'
                    };
                    
                    if (projectInstructions[projectNumber]) {
                        const instructionEl = document.getElementById('instruction-text');
                        if (instructionEl) {
                            instructionEl.style.opacity = '0.5';
                            setTimeout(() => {
                                instructionEl.innerHTML = projectInstructions[projectNumber];
                                instructionEl.style.opacity = '1';
                            }, 200);
                        }
                    }
                });
            }
            
            // Initialize with first tab
            updateInstruction('task1');
            updateProgressIndicator(1, 5);
            
            // Add pulse animation for timer warning
            const style = document.createElement('style');
            style.textContent = `
                @keyframes pulse {
                    0% { opacity: 1; }
                    50% { opacity: 0.7; }
                    100% { opacity: 1; }
                }
                .pulse {
                    animation: pulse 1s infinite;
                }
            `;
            document.head.appendChild(style);
        });
    </script>
    
    
